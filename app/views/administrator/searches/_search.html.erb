<div class="form2">
    <%= form_with url: administrator_search_path, method: :get, local: true do |f| %>
    <spam class="search-button col-xs-12 text-left">
      <%= f.text_field 'search[value]', size: "15x2", class: "fas col-sm-7 pull-left"%>
      <%= button_tag "", :name => nil, class: "fas fa-search" %>
      <%= f.select 'search[how]', options_for_select({ "部分一致" => "partical", "前方一致" => "forward", "後方一致" => "backward", "完全一致" => "match" }) %>
      </spam>
    <% end %>
</div>